{
  "version": 2,
  "functions": {
    "backend/api/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "buildCommand": "npm run build",
  "installCommand": "npm run install:all",
  "framework": "nextjs"
} 